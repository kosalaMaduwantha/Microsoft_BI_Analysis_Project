  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Country">
            <Value>=Parameters!Country.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
SELECT  circuitRef,alternateRaceId
FROM DimDriver dr,DimCircuit ci,FactResults re, DimRace ra,DimDate da, DimCountry co
WHERE  dr.driverSK = re.driverKey AND ci.circuitSK = ra.circuitKey AND ra.raceSK = re.raceKey AND da.DateKey = re.dateResultKey AND co.CountrySK = ci.countryKey
AND country =@Country</CommandText>
      </Query>
      <Fields>
        <Field Name="circuitRef">
          <DataField>circuitRef</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="alternateRaceId">
          <DataField>alternateRaceId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
